			"}",
			"",
			"function soustraire(num1, num2) {",
			"    return num1 - num2;",
			"}",
			"",
			"function additionner(num1, num2) {",
			"    return num1 + num2;",
			"}",
			"",
			"function diviser(num1, num2) {",
			"    return num1 / num2;",
			"}"
		],
